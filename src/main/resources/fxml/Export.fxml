<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressIndicator?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.demon.controller.ExportController"
            prefHeight="150.0" prefWidth="300.0">
    <children>
        <BorderPane layoutX="30.0" layoutY="30.0" style="-fx-background-color: #FFFFFF;" AnchorPane.bottomAnchor="-40.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <center>
                <ProgressIndicator fx:id="downloadProgressIndicator" prefHeight="123.0" prefWidth="300.0" BorderPane.alignment="CENTER" />
            </center>
            <top>
                <HBox alignment="CENTER_LEFT" prefHeight="25.0" style="-fx-background-color: #F8F8F8;" BorderPane.alignment="CENTER">
                    <children>
                        <Label fx:id="exportStatus" text="加载提示中···" />
                    </children>
                    <padding>
                        <Insets left="10.0" />
                    </padding>
                </HBox>
            </top>
            <bottom>
                <HBox alignment="CENTER" prefHeight="20.0" BorderPane.alignment="CENTER">
                    <children>
                        <Button fx:id="downloadYes" disable="true" mnemonicParsing="false" text="确定" />
                    </children>
                    <padding>
                        <Insets bottom="2.0" left="2.0" right="2.0" top="15.0" />
                    </padding></HBox>
            </bottom>
        </BorderPane>
    </children>
</AnchorPane>
